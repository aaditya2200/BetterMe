<?xml version="1.0" encoding="utf-8"?>
<!-- splashScreenR -->
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/splashscree"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true"
    android:background="@drawable/splashscree"
    />


    <!-- drawable/splashscree.xml -->
<vector
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:aapt="http://schemas.android.com/aapt"
android:width="360dp"
android:height="640dp"
android:viewportWidth="360"
android:viewportHeight="640"
    >

<group>

    <clip-path
        android:pathData="M0 0H360V640H0V0Z"
        />

    <path
        android:pathData="M0 0V640H360V0"
        android:fillColor="#FFFFFF"
        />

</group>

</vector>

<View
android:id="@+id/rectangle"
android:layout_width="0dp"
android:layout_height="0dp"
android:layout_alignParentLeft="true"
android:layout_alignParentRight="true"
android:layout_alignParentTop="true"
android:layout_alignParentBottom="true"
android:background="@drawable/rectangle"
    />


    <!-- drawable/rectangle.xml -->
<vector
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:aapt="http://schemas.android.com/aapt"
android:width="300dp"
android:height="300dp"
android:viewportWidth="300"
android:viewportHeight="300"
    >

<group>

    <clip-path
        android:pathData="M0 0H300V300H0V0Z"
        />

</group>

</vector>
